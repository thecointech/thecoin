  "description": "TheCoin simple broker services.  This sample API is an example of how to setup the simplest low-volume exchange, when combined with the included server code, without external dependencies",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn",
    "start": "node index.js",
    "debug": "node --inspect index.js",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "@types/google-cloud__datastore": "^1.3.3",
    "cors": "^2.8.5",
    "esm": "^3.0.84",
    "ethers": "^4.0.27",
    "express": "^4.16.3",
    "jest": "^24.8.0",
    "js-yaml": "^3.3.0",
    "node-mailjet": "^3.3.1",
    "swagger-tools": "0.10.4",
    "ts-jest": "^24.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "ts-node": "^8.1.1",
    "typescript": "^3.4.5"
  }
}
