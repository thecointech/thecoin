{
  "name": "thecoin-admin",
  "private": true,
  "productName": "thecoin-admin",
  "version": "0.2.115",
  "description": "Admin: audit TheCoin system ops",
  "main": ".webpack/main",
  "config": {
    "forge": "./config.forge"
  },
  "scripts": {
    "build": "yarn make",
    "dev": "yarn electron-forge start",
    "_dev:live": "cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "cross-env CONFIG_NAME=prodtest yarn dev",
    "prod:beta": "cross-env CONFIG_NAME=prod yarn dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Stephen Taylor",
    "email": "stephen.taylor.dev@gmail.com"
  },
