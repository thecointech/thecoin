{
  "name": "thecoin-admin",
  "private": true,
  "productName": "thecoin-admin",
  "version": "0.2.119",
  "description": "Admin: audit TheCoin system ops",
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "main": ".webpack/main",
  "config": {
    "forge": "./config.forge"
  },
  "type": "commonjs",
  "scripts": {
    "build": "run -T tsc -b",
    "dev": "yarn electron-forge start",
    "_dev:live": "run -T cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "run -T cross-env CONFIG_NAME=prodtest yarn dev",
    "prod:beta": "run -T cross-env CONFIG_NAME=prod yarn dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "28.1.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0"
  },
  "dependencies": {
    "@thecointech/contract-core": "^0.2.119",
    "@thecointech/shared": "^0.2.119",
    "@thecointech/tx-bill": "^0.2.119",
    "@thecointech/tx-deposit": "^0.2.119",
    "@thecointech/tx-etransfer": "^0.2.119",
    "@thecointech/tx-statemachine": "^0.2.119",
    "electron-squirrel-startup": "^1.0.0",
    "semantic-ui-css": "^2.4.1"
  }
}
