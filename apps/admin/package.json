{
  "name": "thecoin-admin",
  "private": true,
  "productName": "thecoin-admin",
  "version": "0.3.13-test.5",
  "description": "Admin: audit TheCoin system ops",
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "main": ".webpack/main",
  "type": "commonjs",
  "config": {
    "forge": "config/forge.config.mjs"
  },
  "scripts": {
    "build": "run -T tsc -b",
    "dev": "yarn _start:dbg",
    "_dev:live": "run -T cross-env CONFIG_NAME=devlive yarn _start:dbg",
    "prod:test": "run -T cross-env CONFIG_NAME=prodtest yarn _start:dbg",
    "prod": "run -T cross-env CONFIG_NAME=prod yarn _start:dbg",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "_start:dbg": "yarn run -T cross-env NODE_ENV=development yarn run-env $(yarn bin electron-forge) start"
  },
  "keywords": [],
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@thecointech/setenv": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "28.1.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0"
  },
  "dependencies": {
    "@thecointech/contract-core": "^0.3.13-test.5",
    "@thecointech/shared": "^0.3.13-test.5",
    "@thecointech/tx-bill": "^0.3.13-test.5",
    "@thecointech/tx-deposit": "^0.3.13-test.5",
    "@thecointech/tx-etransfer": "^0.3.13-test.5",
    "@thecointech/tx-statemachine": "^0.3.13-test.5",
    "electron-squirrel-startup": "^1.0.0",
    "semantic-ui-css": "^2.4.1"
  }
}
