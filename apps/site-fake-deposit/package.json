{
  "name": "@thecointech/site-test-deposit",
  "productName": "THECoin: Website - Test Deposit",
  "private": true,
  "type": "module",
  "version": "0.2.105",
  "description": "THECoin: simple email generator to trigger deposits on testnet",
  "repository": {
    "type": "git",
    "url": "git://github.com/thecointech/thecoin.git"
  },
  "author": "Stephen Taylor",
  "license": "MIT",
  "scripts": {
    "pre_build": "run-p build:*",
    "_build": "yarn cross-env NODE_ENV=production webpack --config ../../libs/site-base/internal/webpack/webpack.prod.babel.js --progress",
    "build:semantic": "node ./internals/buildSemantic.mjs",
    "build:i18n": "run-s extract compile",
    "clean": "run-p clean:*",
    "clean:build": "rimraf ./build",
    "clean:test": "rimraf ./coverage",
    "clean:analyze": "rimraf stats.json",
    "_deploy": "yarn run-env ./deploy.ts",
    "dev": "yarn node internals/server.cjs --port=3004",
    "extract": "formatjs extract \"src/**/!(*.d|*.test).ts*\" --out-file lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "compile": "formatjs compile lang/en.json --out-file src/translations/en.json && formatjs compile lang/fr.json --out-file src/translations/fr.json"
  },
  "dependencies": {
    "@thecointech/site-base": "^0.2.105"
  }
}
