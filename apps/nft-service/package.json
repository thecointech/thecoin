{
  "name": "@thecointech/nft-service",
  "version": "0.2.47",
  "private": true,
  "description": "TheCoin NFT service.  Manage uploads and gassless transactions with the TheCoin NFT contract",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "apps/nft-service"
  },
  "scripts": {
    "start": "node ./build/main.js",
    "dev": "cross-env TS_NODE_PROJECT=../../tsconfig.dev.json node --inspect=9233 -r ts-node/register ./src/main.ts",
    "dev:live": "cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "yarn run-env dotenv_config_path=../../environments/prodtest.env",
    "deploy": "ts-node --skip-project deploy",
    "build": "yarn tsc -b tsconfig.build.json",
    "gen:swagger": "yarn tsoa spec-and-routes",
    "clean": "run-p clean:*",
    "clean:build": "rimraf ./build",
    "clean:test": "rimraf ./coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt",
    "test": "jest --coverage"
  },
  "engines": {
    "node": "14.x.x"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/main.js",
  "dependencies": {
    "@pinata/sdk": "1.1.23",
    "@thecointech/contract-nft": "^0.2.47",
    "@thecointech/logging": "^0.2.41",
    "@thecointech/signers": "^0.2.46",
    "@thecointech/utilities": "^0.2.46",
    "@tsoa/runtime": "^3.8.0",
    "axios": "0.21.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "image-type": "^4.1.0",
    "js-yaml": "^3.13.1",
    "luxon": "1.26.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/multer": "^1.4.5",
    "@types/node-fetch": "^2.5.5",
    "@types/swagger-ui-express": "^4.1.1",
    "jest": "*",
    "tsoa": "^3.8.0"
  }
}
