{
  "name": "@thecointech/nft-service",
  "version": "0.2.81",
  "private": true,
  "type": "module",
  "description": "TheCoin NFT service.  Manage uploads and gassless transactions with the TheCoin NFT contract",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "apps/nft-service"
  },
  "scripts": {
    "start": "run-env ./build/main.js",
    "dev": "yarn run-env --inspect=9233 ./src/main.ts",
    "dev:live": "cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "cross-env CONFIG_NAME=prodtest yarn dev",
    "deploy": "yarn run-env ./deploy.ts",
    "build": "yarn tsc -b tsconfig.build.json",
    "gen:swagger": "yarn tsoa spec-and-routes",
    "clean": "run-p clean:*",
    "clean:build": "rimraf ./build",
    "clean:test": "rimraf ./coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix"
  },
  "engines": {
    "node": "16.x.x"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/main.js",
  "dependencies": {
    "@pinata/sdk": "1.1.23",
    "@thecointech/contract-nft": "^0.2.81",
    "@thecointech/logging": "^0.2.81",
    "@thecointech/signers": "^0.2.81",
    "@thecointech/utilities": "^0.2.81",
    "@tsoa/runtime": "^4.1.0",
    "axios": "0.21.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "image-type": "^5.0.0",
    "js-yaml": "^3.13.1",
    "luxon": "1.26.0",
    "multer": "^1.4.2",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^4.2.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.5",
    "@types/swagger-ui-express": "^4.1.1",
    "jest": "*"
  }
}
