{
  "entryFile": "./src/main.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["./src/controllers/**/*controller.ts"],
  "spec": {
    "basePath": "/api/v1",
    "specVersion": 2,
    "outputDirectory": "./src/api",
    "specMerging": "recursive",
    "spec": {
      "paths": {
        "/nft/uploadAvatar": {
          "post": {
            "consumes": [
              "multipart/form-data"
            ],
            "parameters": [
              {
                "in": "formData",
                "name": "avatar",
                "required": true,
                "type": "file"
              }
            ]
          }
        }
      }
    }
  },
  "routes": {
    "basePath": "/api/v1",
    "routesDir": "./src/routes",
    "middleware": "express"
  },
  "ignore": [
    "**/node_modules/**"
  ]
}
