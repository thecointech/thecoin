{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "App - Development",
      "request": "launch",
      "runtimeArgs": [
        "dev",
        // "--", // Add "--clean-start" to skip seeding dev db
        // "--clean-start"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "App - Dev:Live",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "_dev:live"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "App - Prod:Test",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "prod:test"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "HARVESTER_ALLOW_OVERRIDES": "true",
        "NOTIFY_ON_MODAL_ENCOUNTER": "true",
      }
    },
    {
      "name": "App - Prod",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "prod"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "HARVESTER_ALLOW_OVERRIDES": "true",
        "NOTIFY_ON_MODAL_ENCOUNTER": "true",
        "DO_NOT_SEND_ETRANSFER": "true",
        "HARVESTER_VERBOSE_AGENT": "true",
        "HARVESTER_DRY_RUN": "true",
        "HARVESTER_OVERRIDE_CHQ_BALANCE": "9000",
        "HARVESTER_OVERRIDE_VISA_BALANCE": "{\"balance\":\"7699.11\",\"dueDate\":\"2024-04-12T00:00:00.000-05:00\",\"dueAmount\":\"4986.59\",\"history\":[]}",
      }
    },
    {
      "name": "App - Live",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "_start"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "CONFIG_NAME": "prodbeta",
        "HARVESTER_DEBUG_LIVE": "true",
        "HARVESTER_ENABLE_CONFIG_EXPORT": "true",
        "HARVESTER_ALLOW_OVERRIDES": "true",
        "NOTIFY_ON_MODAL_ENCOUNTER": "true",
        "HARVESTER_VERBOSE_AGENT": "true",
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Harvest - Live",
      "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.sh",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.cmd"
      },
      // runtimeArgs will be passed directly to your Electron application
      "runtimeArgs": [
        "--harvest",
      ],
      "env": {
        "NODE_ENV": "development",
        // "RUN_SCRAPER_HEADLESS": "false",
        "CONFIG_NAME": "prodbeta",
        "NOTIFY_ON_MODAL_ENCOUNTER": "true"
        // DRY RUN
        // "HARVESTER_DRY_RUN": "true",
        // "HARVESTER_OVERRIDE_CHQ_BALANCE": "9000",
        // "HARVESTER_OVERRIDE_VISA_BALANCE": "{\"balance\":\"7699.11\",\"dueDate\":\"2024-04-12T00:00:00.000-05:00\",\"dueAmount\":\"4986.59\",\"history\":[]}",
      },

      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Harvest - ProdTest",
      "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.sh",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.cmd"
      },
      // runtimeArgs will be passed directly to your Electron application
      "runtimeArgs": [
        "--harvest",
      ],
      "env": {
        "RUN_SCRAPER_HEADLESS": "false",
        "CONFIG_NAME": "prodtest",
        "HARVESTER_DRY_RUN": "true",
      },

      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Harvest - DevLive",
      "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.sh",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/../../node_modules/@electron-forge/cli/script/vscode.cmd"
      },
      // runtimeArgs will be passed directly to your Electron application
      "runtimeArgs": [
        "--harvest",
      ],
      "env": {
        "HARVESTER_PROFILE_FOLDER": "${workspaceFolder}/profile",
        "CONFIG_NAME": "devlive",
      },

      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2",
      "request": "launch",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "smartStep": false,
      "args": [
        "test",
        "--watchAll=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ],
      "customDescriptionGenerator": "function (def) { if (this.toString) { const _v = this.toString(); if (_v.indexOf(\"[object Object]\") < 0) return _v; } return def; }"
    }
  ]
}
