{
  "name": "harvester",
  "version": "0.2.125",
  "private": true,
  "description": "Automatically harvest TheCoin",
  "main": ".webpack/main",
  "config": {
    "forge": "./forge.config"
  },
  "type": "commonjs",
  "keywords": [],
  "author": {
    "name": "Stephen Taylor",
    "email": "stephen.taylor.dev@gmail.com"
  },
  "scripts": {
    "dev": "yarn run -T cross-env RUN_SCRAPER_HEADLESS=false electron-forge start",
    "dev:harvest": "electron-forge start -- -- --harvest",
    "_dev:live": "yarn run -T cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "yarn run -T cross-env CONFIG_NAME=prodtest yarn dev",
    "prod:beta": "yarn run -T cross-env CONFIG_NAME=prodbeta yarn dev",
    "prod": "yarn run -T cross-env CONFIG_NAME=prod yarn dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make:prod": "yarn run -T cross-env CONFIG_NAME=prodbeta electron-forge make",
    "make:prodtest": "yarn run -T cross-env CONFIG_NAME=prodtest electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "run -T tsc -b",
    "test": "yarn :jest",
    "scraper:record": "yarn run-env ./src/scraper/main.ts --record"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@thecointech/jestutils": "^0.2.125",
    "@thecointech/setenv": "workspace:^",
    "@timfish/forge-externals-plugin": "https://github.com/FrozenKiwi/forge-externals-plugin-monorepo",
    "@types/node-notifier": "^8.0.2",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "28.1.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "pouchdb-adapter-memory": "^8.0.1"
  },
  "dependencies": {
    "@thecointech/account": "^0.2.125",
    "@thecointech/apis": "^0.2.125",
    "@thecointech/async": "^0.2.125",
    "@thecointech/contract-plugin-converter": "^0.2.125",
    "@thecointech/contract-plugin-shockabsorber": "^0.2.125",
    "@thecointech/contract-plugins": "^0.2.125",
    "@thecointech/fx-rates": "^0.2.125",
    "@thecointech/logging": "^0.2.125",
    "@thecointech/shared": "^0.2.125",
    "@thecointech/signers": "^0.2.125",
    "@thecointech/utilities": "^0.2.125",
    "@xenova/transformers": "^2.14.0",
    "comdb": "^6.0.0-beta",
    "crontab": "^1.4.2",
    "currency.js": "^2.0.4",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-notifier": "^10.0.0",
    "pouchdb": "^8.0.1",
    "puppeteer": "^22.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4"
  }
}
