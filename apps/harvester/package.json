{
  "name": "harvester",
  "private": true,
  "productName": "harvester",
  "version": "0.2.108",
  "description": "Automatically harvest TheCoin",
  "main": ".webpack/main",
  "config": {
    "forge": "./forge.config"
  },
  "type": "commonjs",
  "keywords": [],
  "author": {
    "name": "Stephen Taylor",
    "email": "stephen.taylor.dev@gmail.com"
  },
  "scripts": {
    "record": "ts-node-esm src/scraper/main.ts",
    "replay": "ts-node-esm src/scraper/replay.ts",
    "dev": "electron-forge start",
    "dev:harvest": "electron-forge start -- -- --harvest",
    "_dev:live": "cross-env CONFIG_NAME=devlive yarn dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "cross-env CONFIG_NAME=prodbeta electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "yarn tsc --noEmit",
    "build:app": "yarn tsc --noEmit tsconfig.app.json",
    "test": "yarn run-jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "22.1.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.4.3",
    "node-loader": "^2.0.0",
    "pouchdb-adapter-memory": "^8.0.1",
    "style-loader": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "windows-scheduler": "1.0.1"
  },
  "dependencies": {
    "@thecointech/apis": "^0.2.108",
    "@thecointech/contract-plugins": "^0.2.108",
    "comdb": "^6.0.0-beta",
    "currency.js": "^2.0.4",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "pouchdb": "^8.0.1",
    "puppeteer": "^19.6.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4"
  }
}
