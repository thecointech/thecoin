{
  "name": "harvester",
  "productName": "harvester",
  "version": "0.3.4",
  "private": true,
  "description": "Automatically harvest TheCoin",
  "main": ".webpack/main",
  "type": "module",
  "keywords": [],
  "author": {
    "name": "Stephen Taylor",
    "email": "stephen.taylor.dev@gmail.com"
  },
  "scripts": {
    "dev:harvest": "electron-forge start -- -- --harvest",
    "dev": "yarn run -T cross-env CONFIG_NAME=development yarn _start:dbg",
    "_dev:live": "yarn run -T cross-env CONFIG_NAME=devlive yarn _start:dbg",
    "prod:test": "yarn run -T cross-env CONFIG_NAME=prodtest yarn _start:dbg",
    "prod:beta": "yarn run -T cross-env CONFIG_NAME=prodbeta yarn _start:dbg",
    "prod": "yarn run -T cross-env CONFIG_NAME=prod yarn _start:dbg",
    "package": "electron-forge package",
    "make:prod": "yarn run -T cross-env CONFIG_NAME=prodbeta yarn _make",
    "make:prodtest": "yarn run -T cross-env CONFIG_NAME=prodtest yarn _make",
    "make:validation": "yarn run -T cross-env CONFIG_NAME=prodbeta NOTIFY_ON_MODAL_ENCOUNTER=true yarn _make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "run -T tsc -b",
    "test": "yarn run :jest",
    "_start:dbg": "yarn run -T cross-env NODE_ENV=development RUN_SCRAPER_HEADLESS=false HARVESTER_PROFILE_FOLDER=\"./.cache/$CONFIG_NAME\" yarn run-env $(yarn bin electron-forge) start",
    "_make": "yarn run-env $(yarn bin electron-forge) make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@thecointech/jestutils": "^0.3.3",
    "@thecointech/setenv": "workspace:^",
    "@timfish/forge-externals-plugin": "https://github.com/FrozenKiwi/forge-externals-plugin-monorepo",
    "@types/node-notifier": "^8.0.2",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "28.1.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "pouchdb-adapter-memory": "^8.0.1"
  },
  "dependencies": {
    "@thecointech/account": "^0.3.4",
    "@thecointech/apis": "^0.3.3",
    "@thecointech/async": "^0.3.3",
    "@thecointech/contract-plugin-converter": "^0.3.4",
    "@thecointech/contract-plugin-shockabsorber": "^0.3.4",
    "@thecointech/contract-plugins": "^0.3.4",
    "@thecointech/fx-rates": "^0.3.4",
    "@thecointech/logging": "^0.3.4",
    "@thecointech/scraper": "^0.3.4",
    "@thecointech/scraper-agent": "^0.3.4",
    "@thecointech/shared": "^0.3.4",
    "@thecointech/signers": "^0.3.4",
    "@thecointech/utilities": "^0.3.4",
    "comdb": "^6.0.0-beta",
    "crontab": "^1.4.2",
    "currency.js": "^2.0.4",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-notifier": "^10.0.0",
    "pouchdb": "^8.0.1",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4"
  }
}
