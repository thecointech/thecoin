{
  "name": "harvester",
  "private": true,
  "productName": "harvester",
  "version": "0.2.116",
  "description": "Automatically harvest TheCoin",
  "main": ".webpack/main",
  "config": {
    "forge": "./forge.config"
  },
  "type": "commonjs",
  "keywords": [],
  "author": {
    "name": "Stephen Taylor",
    "email": "stephen.taylor.dev@gmail.com"
  },
  "scripts": {
    "scraper": "run-env src/scraper/main.ts",
    "dev": "yarn run -T cross-env RUN_SCRAPER_HEADLESS=false electron-forge start",
    "dev:harvest": "electron-forge start -- -- --harvest",
    "_dev:live": "yarn run -T cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "yarn run -T cross-env CONFIG_NAME=prodtest yarn dev",
    "prod:beta": "yarn run -T cross-env CONFIG_NAME=prodbeta yarn dev",
    "prod": "yarn run -T cross-env CONFIG_NAME=prod yarn dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "yarn run -T cross-env CONFIG_NAME=prodbeta electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "run -T tsc -b",
    "test": "run :jest",
    "test:record": "yarn run-env ./src/scraper/main.ts --record"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@electron-forge/plugin-webpack": "^6.2.1",
    "@thecointech/jestutils": "workspace:^",
    "@types/node-notifier": "^8.0.2",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "25.3.2",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "pouchdb-adapter-memory": "^8.0.1"
  },
  "dependencies": {
    "@thecointech/account": "workspace:^",
    "@thecointech/apis": "workspace:^",
    "@thecointech/async": "workspace:^",
    "@thecointech/contract-plugin-converter": "workspace:^",
    "@thecointech/contract-plugin-shockabsorber": "workspace:^",
    "@thecointech/contract-plugins": "workspace:^",
    "@thecointech/fx-rates": "workspace:^",
    "@thecointech/logging": "workspace:^",
    "@thecointech/shared": "workspace:^",
    "@thecointech/signers": "workspace:^",
    "@thecointech/utilities": "workspace:^",
    "comdb": "^6.0.0-beta",
    "currency.js": "^2.0.4",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-notifier": "^10.0.0",
    "pouchdb": "^8.0.1",
    "puppeteer": "^19.6.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4"
  }
}
