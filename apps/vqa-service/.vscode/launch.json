    "version": "0.2.0",
    "configurations": [
        {
            "name": "API: ProdTest",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/bootstrap.py",
            "cwd": "${workspaceFolder}/src",
            "env": {
                "CONFIG_NAME": "prodtest"
            }
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
    ]
}
