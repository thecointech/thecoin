{
  "name": "@thecointech/tx-processor",
  "version": "0.2.115",
  "private": "true",
  "type": "module",
  "description": "The Coin: A command-line app for completing transactions",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "dev": "run-env ./src/main.ts",
    "dev:live": "cross-env CONFIG_NAME=devlive yarn dev",
    "prod:test": "cross-env CONFIG_NAME=prodtest yarn dev",
    "prod": "cross-env CONFIG_NAME=prod yarn start",
    "start": "cross-env CONFIG_NAME=prod yarn run-env ./build/main.js",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js"
  },
  "dependencies": {
    "@thecointech/logging": "^0.2.115",
    "@thecointech/setenv": "^0.2.115",
    "@thecointech/tx-bill": "^0.2.115",
    "@thecointech/tx-deposit": "^0.2.115",
    "@thecointech/tx-etransfer": "^0.2.115",
    "@thecointech/tx-plugins": "^0.2.115",
    "open": "^8.2.1"
  },
  "devDependencies": {
    "@thecointech/site-app": "^0.2.115",
    "jest": "*"
  }
}
