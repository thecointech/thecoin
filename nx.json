{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "package",
          "make",
          "publish",
          "deploy",
          "deploy:rebuild",
          "build:semantic",
          "build:i18n"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/./build"
      ]
    },
    "dev:live": {
      "dependsOn": [
        "^dev:live"
      ]
    }
  }
}
