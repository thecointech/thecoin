{
  "name": "@thecointech/contract-nft",
  "version": "0.2.93",
  "description": "The Coin: Solidity contract for TheCoin NFT",
  "type": "module",
  "keywords": [
    "TheCoin",
    "ethereum",
    "NFT",
    "CO2 neutral"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/contract-nft"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run-s build:contracts build:types build:copy build:src",
    "build:contracts": "yarn _truffle compile",
    "build:types": "yarn contract-gen-types",
    "build:src": "yarn tsc --project tsconfig.build.json",
    "build:copy": "cpx \"./src/deployed\" ./build/",
    "clean": "run-p clean:*",
    "clean:src": "rimraf ./build",
    "clean:contracts": "rimraf ./src/contracts",
    "clean:types": "rimraf ./src/types && rimraf ./migrations/types",
    "test:contract": "yarn _truffle test",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "prod:test": "cross-env CONFIG_NAME=prodtest yarn _deploy",
    "dev:live": "yarn wait-port 9545 && cross-env CONFIG_NAME=devlive yarn _deploy:polygon",
    "_truffle": "run-env exec=truffle --config ./truffle-config.cjs",
    "_deploy": "runs-s _deploy:*",
    "_deploy:polygon": "yarn _truffle migrate --network polygon && yarn verify polygon",
    "_deploy:ethereum": "yarn _truffle migrate --network ethereum && yarn verify ethereum",
    "verify": "run-env ./internal/verify.ts"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": {
      "development": "./build/index_mocked.js",
      "default": "./build/index.js"
    }
  },
  "types": "./build/index.d.ts",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/truffle-upgrades": "^1.14.0",
    "@thecointech/contract-core": "^0.2.93",
    "@thecointech/contract-tools": "^0.2.93",
    "@thecointech/signers": "^0.2.93",
    "@types/bs58": "^4.0.1",
    "jest": "*"
  },
  "dependenciesComments": {
    "@thecointech/contract-core": [
      "We do not have an actual dependency, but truffle can",
      "break on simultaneous compilation so we list this so",
      "lerna doesn't try and build both at the same time"
    ]
  },
  "dependencies": {
    "@thecointech/contract-base": "^0.2.93",
    "@thecointech/utilities": "^0.2.93",
    "bs58": "^4.0.1"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
