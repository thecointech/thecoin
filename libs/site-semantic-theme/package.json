{
  "name": "@thecointech/site-semantic-theme",
  "productName": "THECoin: Website - Semantic Theme",
  "private": true,
  "type": "module",
  "version": "0.5.1",
  "description": "THECoin: Future-proof currency: Semantic theme for websites",
  "repository": {
    "type": "git",
    "url": "git://github.com/thecointech/thecoin.git"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "author": "Stephen Taylor",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prebuild": "run -T rimraf build",
    "build": "run -T tsc -b"
  },
  "exports": {
    "./build": "./internal/buildSemantic.ts",
    "./webpack.less": "./internal/webpack.less.ts",
    "./variables.less": "./src/variables.less",
    "./semantic.less": "./src/semantic.less",
    "./variables": "./src/variables.ts"
  },
  "typesVersions": {
    "*": {
      "build": [
        "internal/buildSemantic.ts"
      ],
      "webpack.less": [
        "internal/webpack.less.ts"
      ],
      "*": [
        "src/*"
      ]
    }
  },
  "dependencies": {
    "fontsource-abril-fatface": "3.1.5",
    "fontsource-montserrat": "3.1.5"
  },
  "devDependencies": {
    "semantic-ui-less": "2.5.0"
  }
}
