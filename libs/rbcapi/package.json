  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "development": "./build/index_mocked.js",
      "devlive": "./build/index_mocked.js",
      "prodtest": "./build/index_mocked.js",
      "default": "./build/index.js"
    }
  },
  "types": "./build/index.d.ts",
  "dependencies": {
    "@thecointech/bank-interface": "^0.2.101",
    "@thecointech/logging": "^0.2.101",
    "@thecointech/store": "^0.2.101",
    "@thecointech/utilities": "^0.2.101",
    "csvtojson": "^2.0.10",
    "promise.any": "^2.0.4",
    "puppeteer": "^19.6.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1"
  },
  "devDependencies": {
    "@thecointech/types": "^0.2.101",
    "@types/promise.any": "^2.0.0",
    "jest": "*"
  },
