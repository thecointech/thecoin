{
  "name": "@thecointech/nft-contract",
  "version": "0.2.18",
  "description": "The Coin: Solidity contract for TheCoin NFT",
  "keywords": [
    "TheCoin",
    "ethereum",
    "NFT",
    "CO2 neutral"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/nft-contract"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "prod:test:migrate": "cross-env CONFIG_NAME=prodtest truffle migrate --network prodtest",
    "build": "run-s build:contracts generate-types build:copy build:src",
    "build:contracts": "truffle compile",
    "build:src": "yarn tsc --project tsconfig.build.json",
    "build:copy": "cpx \"./src/deployed\" ./build/ && cpx \"./src/types/*.d.ts\" ./build/types/",
    "clean": "run-p clean:*",
    "clean:src": "rimraf ./build",
    "clean:contracts": "rimraf ./src/contracts",
    "clean:types": "rimraf ./src/types && rimraf ./migrations/types",
    "test:contract": "truffle test",
    "test": "yarn jest",
    "dev:live": "cross-env CONFIG_NAME=devlive yarn dev:live:migrate",
    "dev:live:emulator": "start yarn truffle develop",
    "dev:live:migrate": "yarn wait-port 9545 && yarn truffle migrate --network devlive",
    "generate-types": "run-p generate-types:*",
    "generate-types:truffle": "yarn typechain --out-dir ./migrations/types --target=truffle-v5 \"src/contracts/*.json\"",
    "generate-types:ethers": "yarn typechain --out-dir ./src/types --target=ethers-v5 \"src/contracts/*.json\"",
    "prod:console": "cross-env CONFIG_NAME=prodtest truffle console --network prodtest"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": {
      "development": "./build/index_mocked.js",
      "default": "./build/index.js"
    }
  },
  "types": "./build/index.d.ts",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/truffle-upgrades": "^1.5.0",
    "@thecointech/contract": "^0.2.18",
    "@thecointech/signers": "^0.2.18",
    "@thecointech/utilities": "^0.2.18",
    "@truffle/hdwallet-provider": "^1.2.3",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/truffle-v5": "^5.0.0",
    "@types/bs58": "^4.0.1",
    "jest": "*",
    "truffle": "^5.1.67",
    "typechain": "^5.1.2"
  },
  "dependenciesComments": {
    "@thecointech/contract": [
      "We do not have an actual dependency, but truffle can",
      "break on simultaneous compilation so we list this so",
      "lerna doesn't try and build both at the same time"
    ]
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "ethers": "^5.4.1"
  },
  "gitHead": "a21ed03dbbd7b1c6263fe4accee2f531e33c7acf"
}
