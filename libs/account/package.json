{
  "name": "@thecointech/account",
  "version": "0.2.113",
  "description": "TheCoin: Account definition & storage",
  "type": "module",
  "keywords": [
    "TheCoin",
    "S&P500"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/account"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "yarn tsc --project tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": "./build/index.js",
    "./store": {
      "development": "./build/development/store.js",
      "devlive": "./build/devlive/store.js",
      "default": "./build/store.js"
    },
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/contract-core": "^0.2.113",
    "@thecointech/fx-rates": "^0.2.113",
    "@thecointech/idx": "^0.2.113",
    "@thecointech/logging": "^0.2.113",
    "@thecointech/signers": "^0.2.113",
    "@thecointech/tx-blockchain": "^0.2.113",
    "@thecointech/utilities": "^0.2.113"
  },
  "devDependencies": {
    "jest": "*"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
