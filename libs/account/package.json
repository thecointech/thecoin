{
  "name": "@thecointech/account",
  "version": "0.5.3",
  "description": "TheCoin: Account definition & storage",
  "type": "module",
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/account"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "yarn run -T rimraf ./build/.buildinfo",
    "build": "run -T tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "run :jest",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": "./build/index.js",
    "./store": {
      "development": "./mocks/development/store.ts",
      "devlive": "./mocks/devlive/store.ts",
      "prodtest": "./mocks/prodtest/store.ts",
      "default": "./build/store.js"
    },
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/contract-core": "^0.5.3",
    "@thecointech/fx-rates": "^0.5.3",
    "@thecointech/idx": "^0.5.3",
    "@thecointech/logging": "^0.5.3",
    "@thecointech/signers": "^0.5.3",
    "@thecointech/tx-blockchain": "^0.5.3",
    "@thecointech/utilities": "^0.5.3"
  },
  "gitHead": "020dc77a4f35658ec7616021248b732b11c9ae0e"
}
