{
  "name": "@thecointech/scraper-testing",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "description": "The Coin: Testing utilities for scraper",
  "keywords": [
    "TheCoin",
    "puppeteer"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/scraper-testing"
  },
  "scripts": {
    "build": "run -T tsc -b",
    "clean": "run -T rimraf ./build && run -T rimraf ./coverage",
    "test": "run :jest"
  },
  "dependencies": {
    "@thecointech/logging": "workspace:^",
    "@thecointech/scraper": "workspace:^"
  },
  "devDependencies": {
    "@thecointech/jestutils": "^0.4.0",
    "@thecointech/setenv": "workspace:^",
    "open": "^8.4.0"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "node": "./build/index.js"
    }
  },
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      "getTestData": [
        "build/index.d.ts"
      ]
    }
  }
}
