{
  "name": "@thecointech/tx-reconciliation",
  "version": "0.5.2",
  "private": true,
  "description": "The Coin: Process reconciliation between bank, blockchain, database and emails to ensure all transactions are completed appropriately",
  "type": "module",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "run -T tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "run :jest",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts",
        "build/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/broker-db": "^0.5.2",
    "@thecointech/tx-blockchain": "^0.5.2",
    "@thecointech/tx-gmail": "^0.5.2",
    "@thecointech/utilities": "^0.5.2",
    "fastest-levenshtein": "^1.0.12"
  },
  "devDependencies": {
    "@thecointech/bank-interface": "^0.5.1"
  }
}
