{
  "name": "@thecointech/tx-reconciliation",
  "private": "true",
  "version": "0.2.42",
  "description": "The Coin: Process reconciliation between bank, blockchain, database and emails to ensure all transactions are completed appropriately",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "jest --coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts",
        "build/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/bank-interface": "^0.2.42",
    "@thecointech/broker-db": "^0.2.42",
    "@thecointech/shared": "^0.2.42",
    "@thecointech/tx-blockchain": "^0.2.42",
    "@thecointech/tx-gmail": "^0.2.42",
    "@thecointech/utilities": "^0.2.42"
  },
  "devDependencies": {
    "jest": "*"
  }
}
