{
  "name": "@thecointech/tx-statemachine",
  "version": "0.2.116",
  "description": "The Coin: The execution statemachine for processing coin transactions",
  "type": "module",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/tx-statemachine"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run -T tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "run :jest",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./transitions": "./build/transitions/index.js",
    "./transitions/*": "./build/transitions/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "build/transitions/*": [
        "build/transitions/*.d.ts"
      ],
      "*": [
        "build/*.d.ts",
        "build/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/async": "workspace:^",
    "@thecointech/broker-db": "workspace:^",
    "@thecointech/contract-core": "workspace:^",
    "@thecointech/contract-plugin-converter": "workspace:^",
    "@thecointech/email": "workspace:^",
    "@thecointech/fx-rates": "workspace:^",
    "@thecointech/logging": "workspace:^",
    "@thecointech/market-status": "workspace:^",
    "@thecointech/utilities": "workspace:^"
  },
  "devDependencies": {
    "@thecointech/bank-interface": "workspace:^",
    "@thecointech/tx-gmail": "workspace:^"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
