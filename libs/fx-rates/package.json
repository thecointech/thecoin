{
  "name": "@thecointech/fx-rates",
  "version": "0.2.17",
  "description": "TheCoin: Client library for fetching fx-rates",
  "keywords": [
    "TheCoin",
    "S&P500"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/fx-rates"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "yarn tsc --project tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "jest --coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "development": "./build/development/index.js",
      "default": "./build/index.js"
    }
  },
  "types": "build/index.d.ts",
  "dependencies": {
    "@thecointech/logging": "^0.2.16",
    "@thecointech/pricing": "*"
  },
  "devDependencies": {
    "jest": "*"
  }
}
