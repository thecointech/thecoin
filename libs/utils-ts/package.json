{
  "name": "@thecointech/utilities",
  "version": "0.2.22",
  "description": "The Coin: Utilities shared amongst typescript-based projects",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/utils-ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "jest --coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts",
        "build/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/logging": "^0.2.22",
    "base32": "^0.0.6",
    "decimal.js-light": "^2.5.1"
  },
  "peerDependencies": {
    "axios": "*",
    "luxon": "*"
  },
  "devDependencies": {
    "@thecointech/types": "^0.2.16",
    "jest": "*"
  },
  "gitHead": "a21ed03dbbd7b1c6263fe4accee2f531e33c7acf"
}
