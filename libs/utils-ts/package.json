{
  "name": "@thecointech/utilities",
  "version": "0.2.117",
  "description": "The Coin: Utilities shared amongst typescript-based projects",
  "type": "module",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/utils-ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run -T tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "run :jest",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*.d.ts",
        "build/*/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@thecointech/logging": "^0.2.117",
    "base32": "^0.0.6",
    "decimal.js-light": "^2.5.1",
    "ethers": "^5.7.0"
  },
  "devDependencies": {
    "@thecointech/jestutils": "^0.2.117",
    "@thecointech/types": "^0.2.117"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
