{
  "name": "@thecointech/scraper",
  "version": "0.2.135",
  "private": true,
  "type": "module",
  "description": "The Coin: Automated scraper for TheCoin",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/scraper"
  },
  "scripts": {
    "build": "run -T tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "run :jest",
    "getTestPages": "yarn run -T cross-env CONFIG_NAME=prod URL_SEQ_LOGGING=false TC_LOG_FOLDER=./logs run-env tools/getTestPages/main.ts",
    "testFailure": "yarn run -T cross-env CONFIG_NAME=prod URL_SEQ_LOGGING=false TC_LOG_FOLDER=false run-env tools/testFailure/main.ts"
  },
  "dependencies": {
    "@web-std/file": "^3.0.3",
    "@xenova/transformers": "^2.14.0",
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^22.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": "./build/index.js",
    "./puppeteer": "./build/puppeteer-init/index.js",
    "./*": "./build/*.js"
  },
  "typesVersions": {
    "*": {
      "puppeteer": [
        "build/puppeteer-init/index.d.ts"
      ],
      "*": [
        "build/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/pixelmatch": "^5.2.6",
    "fastest-levenshtein": "^1.0.16"
  }
}
