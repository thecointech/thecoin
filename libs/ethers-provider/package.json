{
  "name": "@thecointech/ethers-provider",
  "version": "0.2.109",
  "description": "TheCoin: A custom ethers.js provider with access to Polygon and improved Etherscan support",
  "keywords": [
    "ethereum",
    "etherjs"
  ],
  "type": "module",
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/ethers-provider"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run-p build:*",
    "_prebuild:cjs": "rimraf ./build/cjs/**.cjs",
    "_build:cjs": "yarn tsc --project tsconfig.build.cjs.json",
    "_postbuild:cjs": "node ./postbuild.cjs",
    "build:mjs": "yarn tsc --project tsconfig.build.mjs.json",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "clean": "rimraf ./build"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/mjs/node/remote.js",
  "exports": {
    ".": {
      "node": {
        "development": "./build/mjs/node/mocked.js",
        "devlive": "./build/mjs/node/local.js",
        "default": "./build/mjs/node/remote.js"
      },
      "browser": {
        "development": "./build/mjs/web/mocked/index.js",
        "devlive": "./build/mjs/web/local/index.js",
        "default": "./build/mjs/web/remote/index.js"
      }
    },
    "./Erc20Provider": {
      "development": {
        "node": "./build/mjs/node/mocked.js",
        "default": "./build/mjs/web/mocked/index.js"
      },
      "devlive": "./build/mjs/web/local/index.js",
      "default": "./build/mjs/web/remote/index.js"
    },
    "./Erc20Provider/web": "./build/mjs/web/remote/index.js"
  },
  "types": "./build/mjs/index.d.ts",
  "typesVersions": {
    "*": {
      "Erc20Provider": [
        "build/mjs/web/remote/index.d.ts"
      ],
      "Erc20Provider/web": [
        "build/mjs/web/remote/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.0",
    "@thecointech/logging": "^0.2.109"
  },
  "devDependencies": {
    "@thecointech/jestutils": "^0.2.109",
    "@thecointech/setenv": "^0.2.109",
    "jest": "*",
    "renamer": "^4.0.0"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
