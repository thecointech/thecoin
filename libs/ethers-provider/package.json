{
  "name": "@thecointech/ethers-provider",
  "version": "0.2.52",
  "private": true,
  "description": "TheCoin: A custom ethers.js provider with access to Polygon and improved Etherscan support",
  "keywords": [
    "ethereum",
    "etherjs"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/ethers-provider"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "yarn tsc",
    "clean": "rimraf ./build"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "development": "./build/development/index.js",
      "devlive": "./build/devlive/index.js",
      "default": "./build/prod/index.js"
    }
  },
  "types": "./build/prod/index.d.ts",
  "dependencies": {
    "@ethersproject/providers": "*",
    "@thecointech/logging": "*"
  },
  "devDependencies": {
    "@thecointech/signers": "^0.2.52",
    "jest": "*"
  }
}
