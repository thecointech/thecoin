{
  "name": "@thecointech/contract-core",
  "version": "0.2.81",
  "description": "The Coin: Solidity contract for TheCoin currency",
  "type": "module",
  "keywords": [
    "TheCoin",
    "ethereum"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/contract"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run-s build:contracts build:types build:src",
    "build:contracts": "yarn _truffle compile",
    "build:types": "yarn contract-gen-types",
    "build:src": "run-p build:src:*",
    "_build:src:cjs": "yarn tsc --project tsconfig.build.cjs.json",
    "build:src:mjs": "yarn tsc --project tsconfig.build.mjs.json",
    "build:src:copy": "cpx \"./src/deployed/**\" ./build/cjs/deployed && cpx \"./src/deployed/**\" ./build/mjs/deployed",
    "clean": "run-p clean:*",
    "clean:src": "rimraf ./build",
    "clean:contracts": "rimraf ./src/contracts",
    "clean:types": "rimraf ./src/types && rimraf ./migrations/types",
    "test": "yarn run-jest",
    "test:contract": "yarn _truffle test",
    "prod:test": "cross-env CONFIG_NAME=prodtest yarn _deploy",
    "dev:live": "yarn wait-port 9545 && cross-env CONFIG_NAME=devlive yarn _deploy:polygon",
    "prod": "cross-env CONFIG_NAME=prod yarn _deploy:polygon",
    "_truffle": "run-env exec=truffle --config ./truffle-config.cjs",
    "_deploy": "run-s _deploy:*",
    "_deploy:polygon": "yarn _truffle migrate --network polygon && yarn seed",
    "_deploy:ethereum": "yarn _truffle migrate --network ethereum",
    "mint": "run-env ./mint/main.ts",
    "seed": "run-env ./seed/main.ts",
    "seed:fetch": "cross-env CONFIG_NAME=prodbeta yarn run-env ./seed/clone/fetch.ts",
    "verify": "run-env ./internal/verify.ts"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    "./contract": "./build/mjs/index.js",
    ".": {
      "development": "./build/mjs/index_mocked.js",
      "electron": "./build/mjs/index.js",
      "default": "./build/mjs/index.js"
    }
  },
  "types": "./build/mjs/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "build/mjs/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "@openzeppelin/truffle-upgrades": "^1.14.0",
    "@thecointech/async": "^0.2.81",
    "@thecointech/contract-tools": "^0.2.81",
    "jest": "*"
  },
  "dependencies": {
    "@thecointech/contract-base": "^0.2.81"
  },
  "gitHead": "bea153a75a41c0544d4a11901ccade3bfbb2536e"
}
