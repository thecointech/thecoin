{
  "name": "@thecointech/tx-blockchain",
  "private": "true",
  "version": "0.2.102",
  "description": "The Coin: high-level interactions with the ethereum contract",
  "type": "module",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*"
      ]
    }
  },
  "dependencies": {
    "@thecointech/contract-core": "^0.2.102",
    "@thecointech/utilities": "^0.2.102"
  },
  "devDependencies": {
    "jest": "*"
  }
}
