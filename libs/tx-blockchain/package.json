{
  "name": "@thecointech/tx-blockchain",
  "version": "0.2.3",
  "description": "The Coin: high-level interactions with the ethereum contract",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "scripts": {
    "build:lib": "yarn build",
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "jest --coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*.js"
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [ "build/index.d.ts" ],
      "*": [ "build/*" ]
    }
  },
  "dependencies": {
    "@thecointech/contract": "^0.2.3",
    "@thecointech/utilities": "^0.2.3"
  },
  "devDependencies": {
    "jest": "*"
  }
}
