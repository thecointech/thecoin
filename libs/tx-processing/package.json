{
  "name": "@thecointech/tx-processing",
  "version": "0.2.4",
  "description": "The Coin: A library for processing coin deposits and etransfers",
  "keywords": [
    "THECoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/tx-processing"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "run-s clean build publish:clean",
    "publish:clean": "rimraf ./build/.buildinfo",
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "rimraf ./build && rimraf ./coverage",
    "test": "jest --coverage",
    "test:summary": "jest --coverage > ./coverage/summary.txt"
  },
  "files": [
    "build/**/*"
  ],
  "main": "./build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./*": [ "./build/*/index.js" ]
  },
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [ "build/index.d.ts" ],
      "*": [ "build/*.d.ts", "build/*/index.d.ts" ]
    }
  },
  "dependencies": {
    "@thecointech/broker-db": "*",
    "@thecointech/rbcapi": "*",
    "@thecointech/tx-gmail": "^0.2.3",
    "@thecointech/utilities": "^0.2.3"
  },
  "devDependencies": {
    "jest": "*"
  }
}
