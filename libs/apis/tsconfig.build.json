{
  "extends": "../tsconfig.libs.json",
  "compilerOptions": {
    "outDir": "./build/",
    "tsBuildInfoFile": "./build/.buildinfo",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "rootDir": "./src/",
    "baseUrl": "./src/"
  },
  "include": ["src/**/*.ts", "src/**/*.json"],
  "exclude": ["**/*.test.*", "**/__mocks__/**", "**/*.stories.*"],
}
