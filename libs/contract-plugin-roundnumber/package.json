{
  "name": "@thecointech/contract-plugin-roundnumber",
  "version": "0.2.116",
  "description": "The Coin: A sample plugin rounds balance to nearest round number",
  "type": "module",
  "keywords": [
    "TheCoin"
  ],
  "author": "Stephen Taylor <stephen@thecoin.io>",
  "homepage": "http://thecoin.io",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecointech/thecoin.git",
    "directory": "libs/contract-plugin-roundnumber"
  },
  "scripts": {
    "prepublishOnly": "rimraf ./build/.buildinfo",
    "build": "run-s build:contracts build:copy build:src",
    "build:contracts": "yarn _hardhat ./scripts/compile.ts",
    "build:src": "yarn tsc --project tsconfig.build.json",
    "build:copy": "cpx \"./src/deployed/**\" ./build/deployed",
    "clean": "run-p 'clean:*'",
    "clean:src": "rimraf ./build",
    "clean:contracts": "rimraf ./src/contracts && rimraf ./src/types",
    "test": "yarn run-jest",
    "test:ci": "yarn run-jest-nix",
    "dev:live": "yarn wait-port 9545 && cross-env CONFIG_NAME=devlive run-s _deploy _seed",
    "prod:test": "cross-env CONFIG_NAME=prodtest run-s _deploy _seed",
    "prod": "cross-env CONFIG_NAME=prod run-s _deploy _seed",
    "_deploy": "yarn _hardhat ./scripts/deploy.ts",
    "_seed": "yarn _hardhat ./scripts/seed.ts",
    "_hardhat": "cross-env HARDHAT_CONFIG=hardhat.config.cts yarn run-env"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": "./build/index.js"
  },
  "types": "./build/index.d.ts",
  "devDependencies": {
    "@thecointech/async": "^0.2.116",
    "@thecointech/contract-tools": "^0.2.116",
    "jest": "*"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "@thecointech/contract-core": "^0.2.116",
    "@thecointech/contract-oracle": "^0.2.116",
    "@thecointech/contract-plugins": "^0.2.116"
  }
}
