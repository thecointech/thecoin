    "glob": "^11.0.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "3.0.0",
    "image-webpack-loader": "^8.0.1",
    "imports-loader": "3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lerna": "^8.0.1",
