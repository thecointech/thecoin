    "wait-port": "^0.2.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-middleware": "5.1.0",
    "webpack-dev-server": "4.2.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.8.0",
