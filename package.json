{
  "name": "@thecointech/the-coin",
  "private": true,
  "workspaces": {
    "packages": [
      "libs/*",
      "apps/*",
      "apis/**/*",
      "tools"
    ]
  },
  "engines": {
    "node": ">=22.14.x"
  },
  "resolutions": {
    "clone-deep": "4.0.1",
    "@graphql-tools/executor": "0.0.20",
    "axios": "1.6.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "@types/react": "19.2.7",
    "yargs": "17.7.2"
  },
  "scripts": {
    "postinstall": "ts-patch install -s && lerna run postinstall",
    "gen:pre": "openapi-generator-cli version-manager set 7.10.0",
    "_gen:api:tapcap-cs": "openapi-generator-cli generate -i apps/tapcap-supplier/api/swagger.json -g csharp-netcore -o apis/tapcap/cs -c apis/tapcap/client-cs-spec.json",
    "gen:api:rates": "openapi-generator-cli generate -i apps/rates-service/src/api/swagger.json -g typescript-axios -o apis/rates/ts -c apis/rates/client-ts-spec.json",
    "_gen:api:rates-cs": "openapi-generator-cli generate -i apps/rates-service/src/api/swagger.json -g csharp-netcore -o apis/rates/cs -c apis/rates/client-cs-spec.json",
    "gen:api:broker": "openapi-generator-cli generate -i apps/broker-service/src/api/swagger.json -g typescript-axios -o apis/broker/ts -c apis/broker/client-ts-spec.json",
    "gen:api:nft": "openapi-generator-cli generate -i apps/nft-service/src/api/swagger.json -g typescript-axios -o apis/nft/ts -c apis/nft/client-ts-spec.json",
    "gen:api:vqa": "openapi-generator-cli generate -i apps/vqa-service/api/swagger.json -g typescript-axios -o apis/vqa/ts -c apis/vqa/client-ts-spec.json",
    "gen:api": "run-p 'gen:api:*'",
    "genapis": "lerna run gen:swagger && cross-env JAVA_OPTS=\"-Dlog.level=warn\" run-s gen:pre gen:api",
    "build:src": "lerna run build",
    "build:storybook": "storybook build",
    "build:all": "run-s clean genapis build:src build:storybook",
    "build": "yarn build:all",
    "re:build": "yarn build:src",
    "emulator:firestore": "yarn firebase emulators:start --only firestore --project broker-cad",
    "emulator:ceramic": "cd libs/IDX && yarn emulator",
    "emulator:blockchain": "cd libs/contract-tools && yarn emulator",
    "emulators": "run-p 'emulator:*'",
    "dev:live:data": "cd tools && yarn pullRates",
    "dev:live:projects": "yarn wait-port 9377 && yarn lerna run dev:live --stream --no-bail",
    "dev:live:finalize": "yarn wait-port 7001 && yarn lerna run dev:live:finalize --stream --no-bail",
    "dev:live": "cross-env-shell CONFIG_NAME=devlive TC_DEVLIVE_LOGGING_ROOT=$INIT_CWD run-p emulators 'dev:live:*'",
    "deploy:prodtest": "cross-env CONFIG_NAME=prodtest yarn _deploy",
    "deploy:prodbeta": "cross-env CONFIG_NAME=prodbeta yarn _deploy",
    "deploy:prod": "cross-env CONFIG_NAME=prod yarn _deploy",
    "pre_deploy": "run-s check-changes build:all",
    "_deploy": "run-s _deploy:lib _deploy:app",
    "_deploy:lib": "lerna publish --yes",
    "_deploy:app": "run-s _deploy:app:rebuild _deploy:app:gcloud",
    "_deploy:app:rebuild": "lerna run deploy:rebuild",
    "_deploy:app:gcloud": "lerna run deploy --stream --concurrency 1",
    "prod:test:bootstrap": "cross-env CONFIG_NAME=prodtest yarn bootstrap",
    "prod:bootstrap": "cross-env CONFIG_NAME=prod yarn bootstrap",
    "bootstrap": "lerna run bootstrap",
    "clean": "run-p clean:src clean:apis",
    "clean:src": "lerna run clean",
    "clean:ceramic": "rimraf ~/.ceramic && ~/.jsipfs",
    "clean:apis": "git clean -qfdx ./apis",
    "clean:storybook": "rimraf ./storybook-static",
    "test": "lerna run test --stream --",
    "storybook": "storybook dev -p 6006",
    "dedup": "yarn yarn-deduplicate -s fewer",
    "check-changes": "git diff-index --patch-with-stat HEAD && git diff-index --quiet HEAD -- "
  },
  "dependencies": {
    "axios": "1.6.5",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "ethers": "6.15.0",
    "firebase": "^8.10.1",
    "invariant": "2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intl": "^6.0.4",
    "react-redux": "^9.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "semantic-ui-react": "3.0.0-beta.2",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@formatjs/cli": "^5.0.2",
